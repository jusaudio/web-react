{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "ordered-imports": false,
    "no-var-requires": false,
    "no-bitwise": false,
    "no-console": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "defaultSeverity": "warn"
}
